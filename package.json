{
  "name": "kit-start",
  "version": "1.6.10",
  "description": "kit-start 用户项目初始化，适用于单页与多页应用",
  "main": "index.js",
  "style": "lib/style.css",
  "files": [
    "lib",
    "src",
    "packages"
  ],
  "config": {
    "port": "8080"
  },
  "scripts": {
    "precommit": "npm run lint-staged",
    "predeploy": "npm run build",
    "deploy": "npm run deployOnly",
    "deployOnly": "git rev-parse --short HEAD | xargs gh-pages -d dist --message",
    "dev": "node build/dev-server.js",
    "start": "npm run dev",
    "build": "node build/build.js",
    "build:report": "npm_config_report=true node build/build.js",
    "preview": "cross-env NODE_ENV=production node build/build.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src static",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint:fix": "eslint --fix --ext .js src mock tests && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "prettier": "prettier --write ./src/**/**/**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jskit/kit-start.git"
  },
  "keywords": [
    "kit",
    "vuex",
    "webpack",
    "ui",
    "element"
  ],
  "author": "cloudyan <1395093509@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jskit/kit-start/issues"
  },
  "homepage": "https://github.com/jskit/kit-start#readme",
